package undirected_3;

import edu.princeton.cs.algs4.*;

public class EdgeWeightedGraph {
	private final int V;
	private int E;
	private Bag<Edge>[] adj;
	
	public EdgeWeightedGraph(int v) {
		this.V = V;
		this.E = 0;
		adj = (Bag<Edge>[]) new Bag[V];
		for(int v = 0; v < V; v++) {
			adj[v] = new Bag<Edge>();
		}
	}
}
