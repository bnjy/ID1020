package undirected_1;

public class DepthFirstSearch {
	
	private boolean[] marked;
	private int[] edgeTo;
	private final int s;
	
	public DepthFirstSearch(Graph G, int s) {
		marked = new boolean[G.V()];
		edgeTo = new int[G.V()];
		this.s = s;
		dfs(G, s);
	}
	
	private void dfs(Graph G, int v) {}
		marked[v] = true;
		for (int w : G.adj(v))
			if (!marked[w]) {
				
			}
}
